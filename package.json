{
  "name": "chillframe",
  "version": "1.0.1",
  "description": "ChillFrame brings some of the most chill Youtube Music Feeds into one window.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron src/main.js",
    "pack": "electron-builder",
    "build": "electron-builder"
  },
  "build": {
    "appId": "nhalstead.chillFrame",
    "productName": "Chill Frame",
    "directories": {
      "output": "build",
      "buildResources": "src/assets"
    },
    "extraFiles": [
      "src/assets"
    ]
  },
  "author": "Noah Halstead",
  "license": "The Unlicense",
  "dependencies": {
    "electron": "^18.3.7",
    "electron-updater": "^4.0.0"
  },
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^21.2.0"
  }
}
